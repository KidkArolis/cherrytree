{
  "name": "cherrytree",
  "version": "2.3.2",
  "description": "Cherrytree - a flexible hierarchical client side router",
  "main": "index",
  "scripts": {
    "build": "./tasks/build.sh",
    "release": "release --build",
    "test": "standard && karma start --single-run --coverage",
    "lint": "standard",
    "test-no-coverage": "karma start --single-run",
    "watch": "DEBUG=1 webpack -w index.js build/standalone.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/QubitProducts/cherrytree.git"
  },
  "author": "Karolis Narkevicius <karolis.n@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QubitProducts/cherrytree/issues"
  },
  "dependencies": {
    "location-bar": "^3.0.1",
    "path-to-regexp": "^1.0.3"
  },
  "keywords": [
    "router",
    "history",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-regenerator-runtime": "^6.5.0",
    "bro-size": "^1.0.0",
    "es6-promise": "^4.0.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-effroi": "0.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.7.0",
    "kn-release": "^1.0.1",
    "mocha": "^3.1.2",
    "referee": "^1.1.1",
    "standard": "^8.5.0",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6",
    "yargs": "^6.4.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "examples/**/dist",
      "docs/**",
      "build/**"
    ],
    "global": [
      "suite",
      "test",
      "beforeEach",
      "afterEach",
      "assert"
    ]
  }
}
